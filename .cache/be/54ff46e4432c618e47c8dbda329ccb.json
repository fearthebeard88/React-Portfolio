ponse=>\r\n        {\r\n            response.json().then(json=>\r\n            {\r\n                this.setState({jokes: json});\r\n            });\r\n        }).catch(err =>\r\n        {\r\n            alert(err.message);\r\n        });\r\n    }\r\n\r\n    componentDidMount()\r\n    {\r\n        fetch('https://official-joke-api.appspot.com/random_joke')\r\n        .then(response => \r\n        {\r\n            response.json().then(json=>\r\n            {\r\n                this.setState({joke: json});\r\n            });\r\n        }).catch(err =>\r\n        {\r\n            alert(err.message);\r\n        });\r\n    }\r\n\r\n    render()\r\n    {\r\n        return (\r\n            <div>\r\n                <h2>Random Joke</h2>\r\n                <Joke joke={this.state.joke} />\r\n                <hr />\r\n                <h3>Want 10 new jokes?</h3>\r\n                <button onClick={this.fetchJokes}>Click Here!</button>\r\n                {\r\n                    this.state.jokes.map(joke =>\r\n                    {\r\n                        return (<Joke key={joke.id} joke={joke} />)\r\n                    })\r\n                }\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default Jokes;"},"lineCount":null}},"error":null,"hash":"7429e7e5a0be9b9d355bbfda3f329138","cacheData":{"env":{}}}